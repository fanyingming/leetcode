class Solution {
public:
	int factorial(int n) {
		int sum = 1;
		for (int i = 2; i <= n; i++)
			sum *= i;
		return sum;
	}
	string getPermutation(int n, int k) {
		if (k > factorial(n))
			return "";
		vector<int> list, res;
		for (int i = 1; i <= n; i++)
			list.push_back(i);
		while(k > 0) {
			int num = factorial(list.size()-1);
			int a = k / num;
			k = k % num;

			if (a > 0) {
				res.push_back(list.at(a));
			//	res.erase();
			}
			
		}
	}
};
